<?xml version="1.0" encoding="UTF-8"?>
<enderio:recipes xmlns:enderio="http://enderio.com/recipes" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://enderio.com/recipes recipes.xsd ">
    <recipe name="Tweak: Stick from Wood" required="false"><disabled /></recipe>
    <recipe name="Tweak: Chest from Wood" required="false"><disabled /></recipe>
    <recipe name="Tweak: Wood Hopper" required="false"><disabled /></recipe>

    <!-- Alloying -->
    <recipe name="Draconium Ingot" required="false">
        <alloying energy="12500" exp="0.5">
            <input name="dustDraconium" amount="2"/>
            <input name="ingotManyullyn"/>
            <input name="ingotMelodicAlloy"/>
            <output name="ingotDraconium"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Electrical Steel" required="true"><disabled /></recipe>
    <recipe name="Alloy: Dark Steel" required="true"><disabled /></recipe>

    <recipe name="Alloy: Electrical Steel, Variant" required="false">
        <alloying energy="12500" exp="0.5">
            <input name="ingotSteel"/>
            <input name="dustAluminum"/>
            <input name="itemSilicon" amount="2"/>
            <output name="ingotElectricalSteel"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Energetic Alloy" required="true">
        <alloying energy="10000" exp="0.7">
            <input name="dustRedstone" amount="2"/>
            <input name="ingotBronze"/>
            <input name="dustGlowstone" amount="2"/>
            <output name="ingotEnergeticAlloy"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Energetic Silver Alloy" required="false">
        <alloying energy="10000" exp="0.7">
            <input name="dustRedstone" amount="2"/>
            <input name="ingotSilver"/>
            <input name="dustGlowstone" amount="2"/>
            <output name="ingotEnergeticSilver"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Vibrant Alloy" required="true">
        <alloying energy="10000" exp="1">
            <input name="ingotEnergeticAlloy"/>
            <input name="dustEnderPearl"/>
            <input name="ingotArlemite"/>
            <output name="ingotVibrantAlloy"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Vivid Alloy" required="false">
        <alloying energy="10000" exp="0.8">
            <input name="ingotEnergeticSilver"/>
            <input name="dustEnderPearl"/>
            <input name="ingotRupee"/>
            <output name="ingotVividAlloy"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Redstone Alloy" required="true">
        <alloying energy="10000" exp="0.5">
            <input name="ingotRedAlloy"/>
            <input name="itemCompressedRedstone"/>
            <input name="itemSilicon" amount="2"/>
            <output name="ingotRedstoneAlloy"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Conductive Iron" required="true">
        <alloying energy="10000" exp="0.5">
            <input name="dustRedstone" amount="4"/>
            <input name="ingotIronCompressed"/>
            <input name="nuggetCopper" amount="3"/>
            <output name="ingotConductiveIron"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Endergy Crystalline Alloy" required="true">
        <alloying energy="12000" exp="0.5">
            <input name="itemPulsatingPowder"/>
            <input name="ingotElectrotineAlloy"/>
            <input name="dustCertusQuartz"/>
            <output name="ingotCrystallineAlloy"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Endergy Melodic Alloy" required="true">
        <alloying energy="20000" exp="0.5">
            <input name="minecraft:chorus_fruit_popped" amount="2"/>
            <input name="ingotEndSteel"/>
            <input name="ingotEnderium"/>
            <output name="ingotMelodicAlloy"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Endergy Stellar Alloy" required="true">
        <alloying energy="20000" exp="0.5">
            <input name="minecraft:nether_star" amount="2"/>
            <input name="ingotFiery" amount="2" />
            <input name="ingotMelodicAlloy"/>
            <output name="ingotStellarAlloy" amount="2"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Dark Steel, Variant" required="false">
        <alloying energy="10500" exp="0.4">
            <input name="ingotSteel"/>
            <input name="gemQuartzBlack"/>
            <input name="obsidian" amount="4"/>
            <output name="ingotDarkSteel"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: End Steel" required="true">
        <alloying energy="25000" exp="0.5">
            <input name="ingotDarkSteel"/>
            <input name="dustEndstone" amount="4"/>
            <input name="dustDraconium"/>
            <output name="ingotEndSteel"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Soularium" required="true">
        <alloying energy="10000" exp="0.7">
            <input name="soulSand" amount="3"/><!-- has no oredict entry -->
            <input name="ingotElectrum"/>
            <input name="dustNetherQuartz" amount="2"/>
            <output name="ingotSoularium"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Enderium (Thermal Expansion)" required="false"><disabled /></recipe>
    <recipe name="Alloy: Enderium Blend" required="false"><disabled /></recipe>
    <recipe name="Alloy: Invar Blend" required="false"><disabled /></recipe>
    <recipe name="Alloy: Lumium Blend" required="false"><disabled /></recipe>
    <recipe name="Alloy: Signalum Blend" required="false"><disabled /></recipe>
    <recipe name="Alloy: Steel" required="false"><disabled /></recipe>
    <recipe name="Alloy: Manyullyn" required="false"><disabled /></recipe>
    <recipe name="Alloy: Enderium Base (Thermal Expansion)" required="false">
        <alloying energy="8000" exp="0.4">
            <input name="ingotConstructionAlloy"/>
            <input name="ingotPlatinum"/>
            <input name="oredict:enderpearl" amount="2"/>
            <output name="ingotEnderium"/>
        </alloying>
    </recipe>

    <recipe name="Alloy: Tough Alloy (Nuclearcraft)" required="false"><disabled /></recipe>
    <recipe name="Alloy: Magnesium Diboride Alloy (Nuclearcraft)" required="false"><disabled /></recipe>
    <recipe name="Alloy: Lithium Manganese Alloy (Nuclearcraft)" required="false"><disabled /></recipe>
    <recipe name="Alloy: EXTREME Alloy (Nuclearcraft)" required="false"><disabled /></recipe>
    <recipe name="Alloy: Thermoconducting Alloy (Nuclearcraft)" required="false"><disabled /></recipe>

</enderio:recipes>
